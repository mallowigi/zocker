{
  "name": "@mallowigi/zocker",
  "description": "Generate realistic test-data from your Zod-Schemas",
  "version": "2.0.6",
  "author": "Loris Sigrist && Elior Boukhobza",
  "license": "MIT",
  "private": false,
  "keywords": [
    "mock-data",
    "zod"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/mallowigi/zocker.git"
  },
  "homepage": "https://zocker.sigrist.dev",
